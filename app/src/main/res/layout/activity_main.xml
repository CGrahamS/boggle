<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.guest.boggle.MainActivity">
    
    <Button
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_marginLeft="10dp"
        android:id="@+id/buttonA"
        android:layout_alignParentTop="true" />

    <Button
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:id="@+id/buttonE"
        android:layout_below="@+id/buttonA"
        android:layout_alignStart="@+id/buttonA"/>

    <Button
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:id="@+id/buttonI"
        android:layout_below="@+id/buttonE"
        android:layout_alignStart="@+id/buttonA"/>

    <Button
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:id="@+id/buttonM"
        android:layout_below="@+id/buttonI"
        android:layout_alignStart="@+id/buttonA"/>

    <Button
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:id="@+id/buttonB"
        android:layout_alignTop="@+id/buttonA"
        android:layout_toRightOf="@+id/buttonA"
        android:layout_toEndOf="@+id/buttonA" />

    <Button
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:id="@+id/buttonF"
        android:layout_alignTop="@+id/buttonE"
        android:layout_toRightOf="@+id/buttonA"
        android:layout_toEndOf="@+id/buttonA" />

    <Button
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:id="@+id/buttonJ"
        android:layout_alignTop="@+id/buttonI"
        android:layout_toRightOf="@+id/buttonA"
        android:layout_toEndOf="@+id/buttonA" />

    <Button
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:id="@+id/buttonN"
        android:layout_alignTop="@+id/buttonM"
        android:layout_toRightOf="@+id/buttonA"
        android:layout_toEndOf="@+id/buttonA" />

    <Button
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:id="@+id/buttonC"
        android:layout_alignBottom="@+id/buttonB"
        android:layout_toRightOf="@+id/buttonB"
        android:layout_toEndOf="@+id/buttonB" />

    <Button
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:id="@+id/buttonG"
        android:layout_alignBottom="@+id/buttonF"
        android:layout_toRightOf="@+id/buttonB"
        android:layout_toEndOf="@+id/buttonB" />

    <Button
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:id="@+id/buttonK"
        android:layout_alignBottom="@+id/buttonJ"
        android:layout_toRightOf="@+id/buttonB"
        android:layout_toEndOf="@+id/buttonB" />

    <Button
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:id="@+id/buttonO"
        android:layout_alignBottom="@+id/buttonN"
        android:layout_toRightOf="@+id/buttonB"
        android:layout_toEndOf="@+id/buttonB" />

    <Button
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:id="@+id/buttonD"
        android:layout_alignBottom="@+id/buttonC"
        android:layout_toRightOf="@+id/buttonC"
        android:layout_toEndOf="@+id/buttonC" />

    <Button
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:id="@+id/buttonH"
        android:layout_alignBottom="@+id/buttonG"
        android:layout_toRightOf="@+id/buttonC"
        android:layout_toEndOf="@+id/buttonC" />

    <Button
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:id="@+id/buttonL"
        android:layout_alignBottom="@+id/buttonK"
        android:layout_toRightOf="@+id/buttonC"
        android:layout_toEndOf="@+id/buttonC" />

    <Button
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:id="@+id/buttonP"
        android:layout_alignBottom="@+id/buttonO"
        android:layout_toRightOf="@+id/buttonC"
        android:layout_toEndOf="@+id/buttonC" />

    <EditText
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:gravity="center"
        android:id="@+id/editText"
        android:layout_below="@+id/buttonM" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="SUBMIT"
        android:id="@+id/submitButton"
        android:layout_below="@+id/editText" />

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/submitButton"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"></GridLayout>


</RelativeLayout>
